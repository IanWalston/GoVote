{
  "name": "govote",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.18.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "express-promise-router": "^2.0.0",
    "pg": "^7.1.0",
    "pg-copy-streams": "^1.2.0",
    "pg-query-stream": "^1.1.1",
    "request": "^2.88.0",
    "unzip": "^0.1.11"
  },
  "scripts": {
    "start": "concurrently \"node scripts/start.js\" \"babel-node server/server.js\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "node scripts/build.js",
    "build:server": "NODE_ENV=production webpack --config config/webpackProdServer.config.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint src/**/*.jsx",
    "etl": "NODE_ENV=development node bin/etl.js",
    "docker:build": "docker-compose build",
    "docker:createTables": "docker-compose exec govote node ./bin/create-tables.js",
    "docker:seed": "docker-compose exec govote node ./bin/etl.js",
    "docker:dev": "docker-compose up -d",
    "docker:stop": "docker-compose stop"
  },
  "devDependencies": {
    "concurrently": "^3.1.0"
  }
}
